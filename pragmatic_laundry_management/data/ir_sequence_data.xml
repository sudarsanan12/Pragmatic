<odoo>
    <data>

        <record id="seq_qc_check" model="ir.sequence">
            <field name="name">Quality Check</field>
            <field name="code">laundry_management.qc</field>
            <field name="prefix">QC</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="sequence_laundry_review" model="ir.sequence">
            <field name="name">Review Sequence</field>
            <field name="code">laundry_management.review</field>
            <field name="prefix">REV</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False" />
        </record>

        <record id="sequence_laundry_cleaner" model="ir.sequence">
            <field name="name">Review Sequence</field>
            <field name="code">laundry_management.cleaner</field>
            <field name="prefix">SER</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False" />
        </record>

    </data>
</odoo>